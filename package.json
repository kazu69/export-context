{
  "name": "export-context",
  "version": "0.0.7",
  "description": "Get the methods that are not exports as context. these methods becomes possible operations, such as unit testing",
  "main": "index.js",
  "scripts": {
    "lint": " xo src/*.js",
    "pretest": "npm run lint && npm run build",
    "test": "NODE_ENV=test nyc ava test/*.js",
    "build": "babel --debug --presets latest -o index.js src/index.js"
  },
  "keywords": [
    "context",
    "expose",
    "test",
    "unit",
    "browser",
    "dom",
    "babel",
    "dom",
    "window",
    "private"
  ],
  "author": {
    "name": "kazu69",
    "url": "https://github.com/kazu69/export-context"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kazu69/export-context.git"
  },
  "bugs": {
    "url": "https://github.com/kazu69/export-context/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-latest": "^6.16.0",
    "lodash": "^4.16.4",
    "nyc": "^10.1.2",
    "sinon": "^1.17.6"
  },
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "deepmerge": "^1.2.0",
    "jquery": "^3.2.1",
    "jsdom": "^9.6.0",
    "jsdom-global": "^2.1.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "no-native-reassign": "warn",
      "no-global-assign": "warn"
    }
  },
  "nyc": {
    "exclude": [
      "example/**/*.js"
    ]
  }
}
